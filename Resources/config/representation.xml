<?xml version="1.0" encoding="UTF-8"?><container xmlns="http://symfony.com/schema/dic/services"           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">    <parameters>        <parameter key="kna_hal.representation_factory.class">Kna\HalBundle\Representation\RepresentationFactory</parameter>        <parameter key="kna_hal.default_representation_provider.class">Kna\HalBundle\Representation\DefaultRepresentationProvider</parameter>    </parameters>    <services>        <service id="kna_hal.representation_factory" class="%kna_hal.representation_factory.class%" public="true" />        <service id="Kna\HalBundle\Representation\RepresentationFactoryInterface" alias="kna_hal.representation_factory" />        <service id="kna_hal.default_representation_provider" class="%kna_hal.default_representation_provider.class%">            <argument type="service" id="kna_hal.representation_factory" />            <tag name="kna_hal.representation_provider" />        </service>    </services></container>